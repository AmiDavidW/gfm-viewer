= content main
  h2 Pages

  ul
    {{range $index, $path := .}}
      li
        a.page href="files/{{$path}}" {{$path}}
    {{end}}

  .panel.panel-default.hidden#md
    .panel-heading
      h3.panel-title#md-title
    .panel-body#md-body

  script src="/js/jquery.min.js"
  script src="/js/main.js"
